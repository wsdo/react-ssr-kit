{
  "presets": [
    [
      "es2015",
      {
        "loose": true
      }
    ],
    "react"
  ],
  "plugins": [
    [
      "react-hot-loader/babel"
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ] // `style: true` 会加载 less 文件
  ]
}
